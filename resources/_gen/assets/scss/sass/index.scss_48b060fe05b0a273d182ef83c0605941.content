*{font-family:Arial,Helvetica,sans-serif}a{text-decoration:none}body{width:100%;box-sizing:border-box;margin:0!important;background:#fdfdfd;background-color:#f6f6f6;background-color:#fffefe;background-color:#fff;background-size:210px;background-image:url(data:image/svg+xml;base64,)}h1{margin:0;padding:0}h2{margin:0;padding:0}h3{margin:0;padding:0}p,b{margin:0;padding:0}hr{margin:0;padding:0;background:#000}ul li{list-style:none}@media screen and (min-width:420px){body{margin:.5rem!important;width:initial}}.homepage__content{margin-top:2rem;margin-left:1rem}.homepage__content p{font-size:1.2rem;line-height:1.8rem}.homepage__hero .homepage__hero__title{background:#fff;border:6px solid cyan;box-shadow:-5px 5px 0 0 red;padding:.4rem;display:inline-block;font-size:2.3rem}.homepage__hero .homepage__hero__title__secondary{background:#fff;display:inline-block;padding:1rem;border:6px solid cyan;box-shadow:-5px 5px 0 0 red;font-size:1.2rem;margin-bottom:2rem}.tech__container{display:flex;flex-direction:column;width:100%;color:#fff}.tech__container .tech__section{display:flex;flex-direction:column;margin-bottom:2rem;border-radius:2px;box-shadow:-5px 5px 0 0 #000;background:linear-gradient(45deg,#ff2eb6 0%,#e39802 100%)}.tech__container .tech__section .tech__section__left{margin:2rem;display:flex;flex-direction:column}.tech__container .tech__section .tech__section__right{display:flex;flex-direction:column}.tech__container .tech__section .tech__title{margin-bottom:1rem;margin-left:1.6rem;padding:.4rem;padding-left:.6rem;box-shadow:-5px 5px 0 0 cyan;border-bottom:2px solid #ff0}.tech__container .tech__section .tech__description{border-bottom:1px solid #000;margin-bottom:1rem;font-size:1rem}.tech__container .tech__section .tech__recommendation{font-size:.8rem;line-height:1.1rem;margin-bottom:1rem}.tech__container .tech__section .tech__project__container{display:flex;flex-direction:column;justify-content:space-around;height:100%}.tech__container .tech__section .tech__project__container .tech__project__item{width:9rem;margin-bottom:1rem;padding:.6rem;background:#fff;border:3px solid rgba(255,46,182,.5);box-shadow:-5px 5px 0 0 red;color:#000}.poison{margin-left:1rem;padding:.4rem;padding-left:.6rem;box-shadow:-5px 5px 0 0 cyan;border-bottom:2px solid #ff0}.project{padding:.6rem;background:#fff;border:3px solid rgba(255,46,182,.5);color:#000}@media screen and (min-width:768px){.homepage__content{margin-top:4rem;margin-left:4rem}.tech__container .tech__section{flex-direction:row;width:40rem}.tech__container .tech__section .tech__section__left{width:22rem}}